<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="widget_spinner_attributes">
        <attr name="hint" format="reference" />
    </declare-styleable>

    <declare-styleable name="WidgetToolbar">
        <attr name="toolbar_type" format="enum">
            <enum name="simple" value="0" />
            <enum name="back_arrow" value="1" />
            <enum name="back_arrow_and_more" value="2" />
            <enum name="back_arrow_and_download" value="3" />
        </attr>
        <attr name="toolbar_color_scheme" format="enum">
            <enum name="transparent" value="0" />
            <enum name="white" value="1" />
        </attr>
        <attr name="toolbar_title" format="string" />
        <attr name="toolbar_end_text" format="string" />
    </declare-styleable>
</resources>